{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./build/docs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "readable",
    "navType": "horizontal",
    "linenums": false,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
	"syntaxTheme": "dark",
	"systemName": "Product.js",
	"inverseNav": true,
	"search": false,
	"sort": "version"
  }
}
